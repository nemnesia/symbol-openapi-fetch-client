{
  "name": "@nemnesia/symbol-openapi-typescript-fetch-client",
  "description": "OpenAPI client for symbol-openapi-typescript-fetch-client",
  "version": "1.0.0",
  "author": "The Symbol Syndicate",
  "contributors": [
    {
      "name": "ccHarvestasya",
      "x.com": "@ccHarvestasya"
    }
  ],
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "require": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/cjs/index.js"
    },
    "default": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/umd/index.js"
    }
  },
  "unpkg": "./dist/umd/index.js",
  "browser": "./dist/umd/index.js",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nemnesia/symbol-openapi-fetch-client.git"
  },
  "bugs": {
    "url": "https://github.com/nemnesia/symbol-openapi-fetch-client/issues"
  },
  "homepage": "https://github.com/nemnesia/symbol-openapi-fetch-client#readme",
  "keywords": [
    "symbol",
    "openapi",
    "typescript",
    "fetch",
    "client",
    "blockchain"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:brw && npm run build:brw:min && npm run terser:es && npm run terser:umd",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:brw": "vite build --mode=development",
    "build:brw:min": "vite build --mode=production",
    "build:watch": "vite build --watch",
    "terser:es": "terser dist/browser/symbol-openapi-fetch-client.es.min.js -c -m -o dist/browser/symbol-openapi-fetch-client.es.min.js",
    "terser:umd": "terser dist/browser/symbol-openapi-fetch-client.umd.min.js -c -m -o dist/browser/symbol-openapi-fetch-client.umd.min.js",
    "test": "vitest run",
    "lint": "eslint --fix \"{src,test,e2e}/**/*.ts\"",
    "format": "prettier --write \"{src,test,e2e}/**/*.ts\"",
    "publish:dryrun": "npm publish --dry-run",
    "publish:release": "npm run build && npm publish",
    "prepublish:check": "npm run lint && npm run format && npm run build && npm test"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.2",
    "rimraf": "^6.1.2",
    "terser": "^5.44.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.14"
  },
  "volta": {
    "node": "20.19.6",
    "yarn": "4.12.0"
  }
}
